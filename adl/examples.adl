module examples {

struct Name {
  String first;
  String last;
};

union Gender {
  Void female;
  Void male;
  String other;
};

struct Person {
  Name name;
  Word8 age;
  Gender gender;
  String role;
};

union Article {
  Void definite;
  Void indefinite;
};

struct Hierarchy {
  Person leader;
  Vector<Hierarchy> underlings;
};

};